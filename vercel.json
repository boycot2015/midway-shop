{
    "version": 2,
    "name": "midway-shop",
    "builds": [
        {
            "src": "bootstrap.js",
            "use": "@vercel/ncc"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "bootstrap.js",
            "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
        }
    ]
}