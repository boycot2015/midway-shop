{
  "name": "midway-shop",
  "version": "1.0.0",
  "description": "基于 Midway、Vue 3 组装的 SSR 框架，简单、易学易用、方便扩展、集成Midway框架，您一直想要的Vue SSR 框架。",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./bootstrap.js",
    "dev": "cross-env NODE_ENV=local midway-bin dev --ts",
    "build": "pnpm run clean && pnpm run build:midway && pnpm run build:web",
    "build:midway": "midway-bin build -c ",
    "bundle": "bundle && npm run build && ncc build bootstrap.js -o build",
    "bundle_start": "cross-env NODE_ENV=production node ./build/index.js",
    "build:web": "pnpm run build:web:client && pnpm run build:web:server",
    "build:web:client": "vite --config web/vite.config.ts build --ssrManifest --outDir ../build/client",
    "build:web:server": "vite --config web/vite.config.ts build --ssr entry-server --outDir ../build/server",
    "clean": "midway-bin clean",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "preinstall": "only-allow pnpm"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@midwayjs/axios": "^3.16.4",
    "@midwayjs/bootstrap": "^3.16.2",
    "@midwayjs/core": "^3.16.2",
    "@midwayjs/info": "^3.16.4",
    "@midwayjs/koa": "^3.16.4",
    "@midwayjs/logger": "^3.4.2",
    "@midwayjs/static-file": "^3.16.4",
    "@midwayjs/typeorm": "^3.16.4",
    "@midwayjs/validate": "^3.16.4",
    "axios": "^1.6.2",
    "cross-env": "^6.0.3",
    "crypto-js": "^4.2.0",
    "element-plus": "^2.7.3",
    "koa-connect": "^2.1.0",
    "mysql": "^2.18.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "only-allow": "^1.2.1",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qs": "^6.11.2",
    "typeorm": "^0.3.20",
    "vite": "^2.9.16",
    "vue": "^3.3.8",
    "vue-qrcode": "^2.2.2",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@midwayjs/bundle-helper": "^1.3.0",
    "@midwayjs/cli": "^2.1.1",
    "@midwayjs/mock": "^3.16.4",
    "@types/jest": "^26.0.24",
    "@types/koa": "^2.13.11",
    "@types/node": "14",
    "@vercel/ncc": "^0.38.1",
    "@vitejs/plugin-vue": "^2.3.4",
    "jest": "^26.6.3",
    "mwts": "^1.3.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "sass": "^1.69.5",
    "ts-jest": "^26.5.6",
    "typescript": "^4.6.4",
    "vite-plugin-compression": "^0.5.1",
    "vue-tsc": "^0.34.17"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "build",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "vue3",
    "midway",
    "midwayjs",
    "element-plus",
    "antdv",
    "antd",
    "typescript",
    "pinia",
    "pnpm"
  ],
  "pnpm": {
    "overrides": {
      "pinia": "^2.1.7"
    }
  },
  "author": "1134573460@qq.com>",
  "license": "MIT"
}
